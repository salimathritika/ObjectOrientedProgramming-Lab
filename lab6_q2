//NEED TO EDIT CODE

class Student {
    private int registrationNumber;
    private String name;
    private int age;

    public Student(int registrationNumber, String name, int age) {
        this.registrationNumber = registrationNumber;
        this.name = name;
        this.age = age;
    }

    public void displayDetails() {
        System.out.println("Registration Number: " + registrationNumber);
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
}

class UGStudent extends Student {
    private int semester;
    private double fees;

    public UGStudent(int registrationNumber, String name, int age, int semester, double fees) {
        super(registrationNumber, name, age);
        this.semester = semester;
        this.fees = fees;
    }

    public void displayUGDetails() {
        System.out.println("UG Student Details:");
        displayDetails();
        System.out.println("Semester: " + semester);
        System.out.println("Fees: $" + fees);
    }
}

class PGStudent extends Student {
    private int semester;
    private double fees;

    public PGStudent(int registrationNumber, String name, int age, int semester, double fees) {
        super(registrationNumber, name, age);
        this.semester = semester;
        this.fees = fees;
    }

    public void displayPGDetails() {
        System.out.println("PG Student Details:");
        displayDetails();
        System.out.println("Semester: " + semester);
        System.out.println("Fees: $" + fees);
    }
}

public class lab6_q2 {
    public static void main(String[] args) {
        // Create UG students
        UGStudent ug1 = new UGStudent(1001, "John Doe", 20, 2, 5000.0);
        UGStudent ug2 = new UGStudent(1002, "Jane Smith", 21, 3, 5500.0);

        // Create PG students
        PGStudent pg1 = new PGStudent(2001, "Alice Johnson", 24, 1, 8000.0);
        PGStudent pg2 = new PGStudent(2002, "Bob Wilson", 25, 2, 7500.0);

        // Display UG student details
        System.out.println("UG Admissions:");
        ug1.displayUGDetails();
        System.out.println();
        ug2.displayUGDetails();
        System.out.println();

        // Display PG student details
        System.out.println("PG Admissions:");
        pg1.displayPGDetails();
        System.out.println();
        pg2.displayPGDetails();
        System.out.println();

        // Calculate total admissions
        int totalUGAdmissions = 2; // Two UG admissions
        int totalPGAdmissions = 2; // Two PG admissions
        System.out.println("Total UG Admissions: " + totalUGAdmissions);
        System.out.println("Total PG Admissions: " + totalPGAdmissions);
    }
}
